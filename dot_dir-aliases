aliasdir() {
if [[ -d "$2" ]]; then
  alias "$1"="cd $2"
fi
}

aliasdir ams "$C_ROOT/src/AssetMediaService"
aliasdir api "$C_ROOT/src/Azure.APIManagement"
aliasdir bsn "$C_ROOT/src/BuildScripts.Net"
aliasdir ca "$C_ROOT/src/Catalogs"
aliasdir cn "$C_ROOT/src/CatalogNexus"
aliasdir de "$WIN_HOME/Desktop"
aliasdir dow "$WIN_HOME/Downloads"
aliasdir dr "$WIN_HOME/Dropbox"
aliasdir es "$C_ROOT/src/EntityStore"
aliasdir exp "$C_ROOT/src/experiment"
aliasdir fl "$C_ROOT/src/Foundation.Logging"
aliasdir fm "$C_ROOT/src/Foundation.Messaging"
aliasdir fo "$C_ROOT/src/Foundation"
aliasdir fw "$C_ROOT/src/Foundation.WebApi"
aliasdir im "$C_ROOT/src/IQ.Messaging"
aliasdir mg "$C_ROOT/src/Monitoring.Graphite"
aliasdir pe "$C_ROOT/src/product-excellence"
aliasdir pl "$C_ROOT/src/ProductLibrary"
aliasdir prj "$C_ROOT/prj"
aliasdir sbm "$C_ROOT/src/ServiceBusMonitor"
aliasdir src "$C_ROOT/src"
